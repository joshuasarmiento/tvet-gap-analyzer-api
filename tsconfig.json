    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./api"
  },
  "include": ["api/**/*"],
  "exclude": ["node_modules","public"]
